{
	"name": "librespot",
	"version": "0.2.7",
	"description": "A FOSS Spotify library.",
	"main": "build/index.js",
	"types": "build/index.d.ts",
	"type": "module",
	"files": [
		"build/**/*",
		"proto/**/*"
	],
	"repository": {
		"type": "git",
		"url": "https://git.gay/h/librespot-js"
	},
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./build/index.d.ts",
			"import": "./build/index.js"
		},
		"./types": {
			"types": "./build/utils/types.d.ts",
			"import": "./build/utils/types.js"
		},
		"./rawtypes": {
			"types": "./build/utils/rawtypes.d.ts",
			"import": "./build/utils/rawtypes.js"
		}
	},
	"author": "hazycora",
	"license": "MIT",
	"dependencies": {
		"fast-xml-parser": "^4.3.2",
		"protobufjs": "^7.2.5",
		"shannon-bindings": "^0.1.0",
		"undici": "^5.27.0"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^6.9.1",
		"@typescript-eslint/parser": "^6.9.1",
		"dotenv": "^16.3.1",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"prettier": "^3.0.3",
		"typescript": "^4.9.5"
	}
}
